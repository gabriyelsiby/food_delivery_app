{
    "version": 1,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/every-minute",
        "dest": "/api/every-minute"
      },
      {
        "src": "/api/every-hour",
        "dest": "/api/every-hour"
      },
      {
        "src": "/api/every-day",
        "dest": "/api/every-day"
      }
    ],
    "crons": [
      {
        "path": "/api/every-minute",
        "schedule": "* * * * *"
      },
      {
        "path": "/api/every-hour",
        "schedule": "0 * * * *"
      },
      {
        "path": "/api/every-day",
        "schedule": "0 0 * * *"
      }
    ]
  }
  